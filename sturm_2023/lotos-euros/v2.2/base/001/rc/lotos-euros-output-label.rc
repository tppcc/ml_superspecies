! ** labeling ouptut

!~~ Labeling output
le.output.label.files           :  label-sfc label-3d
!le.output.label.files           :  

! Which labelled species should be in output
le.output.label.label-sfc.specs         :  ppm_f ppm_c ec_f ec_c so4a_c so4a_f no3a_c no3a_f nh4a_f na_c na_cc na_ccc na_f na_ff pom_c pom_f dust_f dust_ff dust_ccc dust_cc dust_c \
                                           no no2 nh3 so2 hno3 n2o5 pan
le.output.label.label-sfc.levels        :  1
le.output.label.label-sfc.levtype       :  levels
le.output.label.label-sfc.bounding_box  :
le.output.label.label-sfc.dhour         :  ${my.output.dhour}
le.output.label.label-sfc.collect       :  daily

! Which labelled species should be in output
le.output.label.label-3d.specs          :  ppm_f ppm_c ec_f ec_c so4a_c so4a_f no3a_c no3a_f nh4a_f na_c na_cc na_ccc na_f na_ff pom_c pom_f dust_f dust_ff dust_ccc dust_cc dust_c \
                                           no no2 hno3 hno2 pan n2o5 no3 so2 nh3
le.output.label.label-3d.levels         :  all
le.output.label.label-3d.levtype        :  levels
le.output.label.label-3d.bounding_box   :  
le.output.label.label-3d.dhour          :  ${my.output.dhour}
le.output.label.label-3d.collect        :  daily

! labelled tracers for boundary run
le.output.label.label-bound.specs          :  ppm_f ppm_c ec_f ec_c so4a_c so4a_f no3a_c no3a_f nh4a_f na_c na_cc na_ccc na_f na_ff pom_c pom_f dust_f dust_ff dust_ccc dust_cc dust_c \
                                             no no2 hno3 hno2 pan n2o5 no3 so2 nh3
le.output.label.label-bound.levels         :  all
le.output.label.label-bound.levtype        :  levels
le.output.label.label-bound.bounding_box   :  ${le.output.conc.conc-bound.bounding_box}
le.output.label.label-bound.dhour          :  ${my.output.dhour}
le.output.label.label-bound.collect        :  daily

!~~ Labelled concentration output 
